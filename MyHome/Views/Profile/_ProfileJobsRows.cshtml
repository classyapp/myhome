@model IEnumerable<Classy.DotNet.Responses.JobView>

@foreach (var job in Model)
{
    <tr>
        <td>@job.JobId</td>
        <td>@job.CreatedAt</td>
        <td>@(job.UpdatedAt.HasValue ? job.UpdatedAt.Value.ToLongDateString() : string.Empty)</td>
        <td>@(job.Succeeded + job.Failed) / <a href="/profile/job/@job.JobId/errors">@job.Failed</a></td>
        @*<td>@(job.Succeeded + job.Failed) / @(job.Failed == 0 ? "0" : "<a href=\"/profile/jobs?dl=true\">" + job.Failed + "</a>")</td>*@
        <td>@job.Status</td>
    </tr>
}